<%- include('../partials/header') %> 

<form action="/new" method="POST">
    <div class="mb-3">
      <label for="exampleInputEmail1" class="form-label">Character name</label>
      <input type="text" name="characterName" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
    </div>
    <div class="mb-3">
      <label for="exampleInputPassword1" class="form-label">Character image</label>
      <input type="text" name="characterImage" class="form-control" id="exampleInputPassword1">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
    <p><%= title %> </p>
</form>